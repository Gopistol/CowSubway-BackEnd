{
    "env": {
        "browser": true,
        "es2021": true,
        "node" : true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "prettier"],
    "plugins": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error", { "endOfLine": "auto" }
        ],
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off"
    }
}
